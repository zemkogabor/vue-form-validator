{
  "name": "@zemkogabor/vue-form-validator",
  "version": "0.0.3",
  "description": "A lightweight Vue.js library that implements the JavaScript Constraint Validation API, offering advanced form validation with custom error messages and internationalization support.",
  "module": "./dist/vue-form-validator.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/vue-form-validator.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "serve": "vite preview",
    "eslint": "eslint '{**/*,*}.{js,ts,jsx,tsx,vue}' --cache",
    "eslint:fix": "eslint '{**/*,*}.{js,ts,jsx,tsx,vue}' --fix"
  },
  "keywords": [
    "vue.js",
    "form-validaton",
    "constraint-validation-api",
    "validator",
    "custom-validator",
    "i18n",
    "lightweight"
  ],
  "author": "Gabor Zemko <gaborzemko@gmail.com>",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.22.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.8.3",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1",
    "vue-tsc": "^1.8.27"
  }
}
